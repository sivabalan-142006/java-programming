import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); 
        int siva = 0;
        for (int i = 0; i < n; i++)
        {
            int bala = sc.nextInt();
            if (bala > 0) 
            { 
                siva += bala * bala; 
            }
        }
        System.out.println(siva);
    }
}
