import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int s = obj.nextInt();
        int sum = 0;
        for (int i = 1; i < s; i++) {
            if (s % i == 0) {
                sum += i;
            }
        }
         if (sum > s) {
            System.out.printf("Abundant Number");
        } else {
            System.out.printf("Not Abundant Number");
        
         
         }

        
        
        
    }
}
