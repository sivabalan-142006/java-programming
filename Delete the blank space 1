import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String input = sc.nextLine();
                String output = input.replaceAll("\\s+", "");
                System.out.println(output);
    }
}
